<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.game.onecricket">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission tools:node="remove" android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission tools:node="remove" android:name="android.permission.READ_SMS"/>

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES " />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="com.game.onecricket.application.BaseApplication"
        android:allowBackup="false"
        android:debuggable="false"
        android:icon="@drawable/round_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/round_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="HardcodedDebugMode"
        android:requestLegacyExternalStorage="true"
        tools:replace="android:allowBackup,android:icon,android:roundIcon">
        <activity android:name="com.game.onecricket.activity.createactivity"></activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.game.onecricket.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <activity
            android:name="com.game.onecricket.activity.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.game.onecricket.activity.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.RegistrationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.VerifyOTPActivity"
            android:screenOrientation="portrait" />



        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.game.onecricket.activity.ForgotVerifyOTPActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.NewPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.ContestListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.CreateTeamActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.ChooseCandVCActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyTeamListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.JoinContestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyJoinedFixtureContestListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyFixtureContestDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyJoinedLiveContestListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyJoinedResultContestListActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.game.onecricket.activity.MyResultContestDetailsActivity" />
        <activity android:name="com.game.onecricket.activity.MyLiveContestDetailsActivity" />
        <activity android:name="com.game.onecricket.activity.PaymentOptionActivity" />
        <activity android:name="com.game.onecricket.activity.MyAccountActivity" />
        <activity android:name="com.game.onecricket.activity.PaytmActivity" />
        <activity
            android:name="com.paytm.pgsdk.PaytmPGActivity"
            android:configChanges="keyboardHidden|orientation|keyboard"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.EditProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.AddCashActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.MyTransactionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.NotificationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.InviteFriendsActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.game.onecricket.activity.InvitedFriendListActivity" />
        <activity
            android:name="com.game.onecricket.activity.PaymentConfirmationActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.game.onecricket.activity.WebviewAcitivity" />
        <activity android:name="com.game.onecricket.activity.WithdrawAmountActivity" />
        <activity android:name="com.game.onecricket.activity.PayUMoneyPaymentActivity" />
        <activity
            android:name="com.game.onecricket.activity.GlobalRankActivity"
            android:screenOrientation="portrait" />

        <service
            android:name="com.game.onecricket.FirebaseMessagingService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.game.onecricket.FirebaseInstanceIDService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.game.onecricket.activity.UploadKYCActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.CreateContestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.SelectPrizeCreateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.EnterInviteCodeContestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.InviteInContestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.TrakNPayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.game.onecricket.activity.CashfreeActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.game.onecricket.activity.MatchOddsActivity" />
        <activity android:name="com.game.onecricket.chatroom.ChatRoomActivity" />
        <activity
            android:name="com.game.onecricket.activity.MatchOddsTabsActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.game.onecricket.activity.GlobalLeaderActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="ch01"/>
    </application>

</manifest>